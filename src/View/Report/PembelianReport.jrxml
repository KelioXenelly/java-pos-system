	<queryString>
		<![CDATA[SELECT 
    pembelian.pembelian_id,
    pembelian.tanggal_beli,
    pembelian.barang_id,
    barang.nama_barang,
    barang.kode_barang,
    pembelian.qty,
    pembelian.harga_beli,
    pembelian.harga_jual,
    pembelian.profit,
    (pembelian.qty * pembelian.harga_beli) AS harga_pokok
FROM 
    pembelian
LEFT JOIN 
    barang ON pembelian.barang_id = barang.barang_id
LEFT JOIN 
    detailbarang ON barang.barang_id = detailbarang.barang_id;]]>
	</queryString>
